{
    "name": "limsABC connector",
    "version": "1.0",
    "manifest_version":2,
    "description": "Connector for limsABC website",
    "icons": {
        "16" : "images/icon-16.png",
        "32" : "images/icon-32.png",
        "48" : "images/icon-48.png",
        "64" : "images/icon-64.png"
    },
    "app": {
        "background": {
          "scripts": ["js/init-lims.js"]
        }
    },
    "externally_connectable": {
      "matches": ["*://localhost/*","*://lims-local-dev.com/*"]
    },
    "sockets": {
        "tcp": {
            "connect": "*:*"
        }
    }
}